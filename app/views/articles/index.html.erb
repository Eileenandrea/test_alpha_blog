<h1>Articles listing page</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Created at</th>
            <th>Last Update</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% @articles.each do |article|%>
            <tr>
                <td><%= article.title %></td>
                <td><%= article.description %></td>
                <td><%= article.created_at %></td>
                <td><%= article.updated_at %></td>
                <td>Placeholder</td>
            </tr>
        <% end %>
    </tbody>
</table>