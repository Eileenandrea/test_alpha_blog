<h1>Articles listing page</h1>
<table>
    <thead>
        <tr>
             <th>Id</th>
            <th>Title</th>
            <th>Description</th>
            <th>Created at</th>
            <th>Last Update</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% @articles.each do |article|%>
            <tr>
                <td><%= article.id %></td>
                <td><%= article.title %></td>
                <td><%= article.description %></td>
                <td><%= article.created_at %></td>
                <td><%= article.updated_at %></td>
                <td><%= link_to 'Show',show_article_path(article) %></td>
                <td><%= link_to 'Edit',edit_article_path(article) %></td>
                <td><%= link_to 'Delete',delete_article_path(article), method: :delete %></td>
            </tr>
        <% end %>
    </tbody>
</table>
<button><%= link_to 'Create New Blog',new_article_path %>
</button>
